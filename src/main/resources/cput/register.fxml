      </Label>
      <PasswordField fx:id="pwdConfirmPassword" layoutX="93.0" layoutY="459.0" prefHeight="25.0" prefWidth="336.0" promptText="Confirm password" />
      <Label layoutX="92.0" layoutY="494.0" text="University">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <ComboBox fx:id="cmbUniversity" layoutX="93.0" layoutY="519.0" prefHeight="25.0" prefWidth="336.0" promptText="Select...">
      <items>
         <FXCollections fx:factory="observableArrayList">
            <String fx:value="Cape Peninsula University of Technology" />
            <String fx:value="University of Western Cape" />
            <String fx:value="University of Cape Town" />
            <String fx:value="University of South Africa" />
         </FXCollections>
      </items>
      </ComboBox>
      <Button fx:id="btnRegister" layoutX="93.0" layoutY="568.0" mnemonicParsing="false" onAction="#btnRegisterOnAction" prefHeight="25.0" prefWidth="336.0" text="Register">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label fx:id="lblClerkExists" layoutX="137.0" layoutY="133.0" prefHeight="17.0" prefWidth="290.0" textFill="RED" />
