      <TextField fx:id="txtSurname" layoutX="92.0" layoutY="282.0" prefHeight="25.0" prefWidth="336.0" promptText="Surname" />
      <Label layoutX="94.0" layoutY="314.0" text="Phone number">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="txtPhone" layoutX="93.0" layoutY="342.0" prefHeight="25.0" prefWidth="336.0" promptText="Phone number" />
      <Label layoutX="93.0" layoutY="374.0" text="Password">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <PasswordField fx:id="pwdPassword" layoutX="93.0" layoutY="399.0" prefHeight="25.0" prefWidth="336.0" promptText="Password" />
