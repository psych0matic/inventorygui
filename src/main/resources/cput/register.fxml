<AnchorPane prefHeight="657.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cput.RegisterController">
   <children>
      <AnchorPane prefHeight="82.0" prefWidth="520.0" style="-fx-background-color: #E96151;">
         <children>
            <ImageView fx:id="imgShield" fitHeight="42.0" fitWidth="69.0" layoutX="32.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/shield.png" />
               </image>
            </ImageView>
            <Label layoutX="96.0" layoutY="28.0" text="Control Clerk Registration" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="92.0" layoutY="132.0" text="Email">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="txtEmail" layoutX="92.0" layoutY="160.0" prefHeight="25.0" prefWidth="336.0" promptText="Email" />
      <Label layoutX="92.0" layoutY="193.0" text="Name">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="txtName" layoutX="92.0" layoutY="221.0" prefHeight="25.0" prefWidth="336.0" promptText="Name" />
      <Label layoutX="92.0" layoutY="254.0" text="Surname">
         <font>
            <Font size="13.0" />
